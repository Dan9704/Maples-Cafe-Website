\n            this.sucMessage = \"Thank you very much for your feedback. We are striving everyday to provide better services!\"\n            this.resetForm();\n        }\n      },\n      resetForm() {\n        this.rating = null;\n        this.description = '';\n        this.errMessage = '';\n      },\n      // FeedBack Section\n      fetchReviews() {\n        axios\n          .get('https://mercury.swin.edu.au/cos30043/s104357292/HDTASK/Coffee/src/resources/api_review.php/')\n          .then(response => {\n            this.reviews = response.data;\n          })\n          .catch(error => {\n            console.log('Failed to load reviews:', error);\n          });\n      },\n    },\n  };\n  </script>\n  \n  <style scoped>\n  .v-divider {\n    margin: 16px 0;\n  }\n  </style>\n  "],"mappings":"AA2CI,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAO,uCAAuC;AAC9C,OAAO,kCAAkC;AACzC;AACF,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACL;MACAC,WAAW,EAAE,EAAE;MACfC,UAAU,EAAE,EAAE;MACdC,UAAU,EAAE,EAAE;MACd;MACAC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,YAAY,CAAC,CAAC;EACrB,CAAC;EAEDC,OAAO,EAAE;IACP;IACA;IACA;IACA;IACA;IACA;IACA;IACAC,UAAUA,CAAA,EAAG;MACX,IAAI,IAAI,CAACC,QAAQ,CAAC,CAAC,EAAE;QACjB,MAAMC,MAAK,GAAI,6FAA6F;QAC5GC,KAAK,CAACD,MAAM,EAAE;UACVE,MAAM,EAAE,MAAM;UACdC,OAAO,EAAE;YACL,cAAc,EAAE;UACpB,CAAC;UACDC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAAC,IAAI,CAACZ,MAAM;QACpC,CAAC,EACIa,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAChCF,IAAI,CAACjB,IAAG,IAAK;UACVoB,OAAO,CAACC,GAAG,CAAC,2BAA2B,EAAErB,IAAI,CAAC;UAC9C;QACJ,CAAC,EACAsB,KAAK,CAACC,KAAI,IAAK;UACZH,OAAO,CAACG,KAAK,CAAC,qBAAqB,EAAEA,KAAK,CAAC;UAC3C;QACJ,CAAC,CAAC;;QACN,IAAI,CAACpB,UAAS,GAAI,6FAA4F;QAC9G,IAAI,CAACqB,SAAS,CAAC,CAAC;MACpB;IACF,CAAC;IACDA,SAASA,CAAA,EAAG;MACV,IAAI,CAACC,MAAK,GAAI,IAAI;MAClB,IAAI,CAACxB,WAAU,GAAI,EAAE;MACrB,IAAI,CAACC,UAAS,GAAI,EAAE;IACtB,CAAC;IACD;IACAI,YAAYA,CAAA,EAAG;MACbR,KAAI,CACD4B,GAAG,CAAC,6FAA6F,EACjGT,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACS,OAAM,GAAIT,QAAQ,CAAClB