   <v-sheet class=\"reviews-section my-5\">\n      <v-card-item class=\"text-center\">\n        <v-card-title class=\"title-secondary\">Feedback from our most-valued customers</v-card-title>\n      </v-card-item>\n      <!-- from here -->\n      <v-card v-for=\"review in reviews\" :key=\"review.id\" class=\"review-card m-4 p-4\">\n        <v-card-title>Review {{ review.id }}</v-card-title>\n        <v-rating v-model=\"review.rating\" empty-icon=\"mdi-coffee-outline\" full-icon=\"mdi-coffee\" color=\"brown\" readonly>\n        </v-rating>\n        <v-card-text class=\"review-text\">{{ review.description }}</v-card-text>\n      </v-card>\n    </v-sheet>\n  </v-container>\n</template>\n\n<script>\nimport '@mdi/font/css/materialdesignicons.css';\nimport 'bootstrap/dist/css/bootstrap.css';\nimport axios from 'axios'; // Add this line\n\nexport default {\n  name: 'ComponentTwo',\n  data() {\n    return {\n      rating: null,\n      description: '',\n      errMessage: '',\n      sucMessage: '',\n      reviews: [], // Change this to reviews\n    };\n  },\n  mounted() {\n    this.fetchReviews();\n  },\n  methods: {\n    validate() { // Add this method\n      if (!this.rating) {\n        this.errMessage = \"Please provide a rating.\";\n        return false;\n      }\n      return true;\n    },  /* eslint-disable */\n    submitForm() {\n      if (this.validate()) {\n        const apiUrl = 'https://mercury.swin.edu.au/cos30043/s104357292/HDTASK/Coffee/src/resources/api_review.php/';\n        fetch(apiUrl, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json',\n          },\n          body: JSON.stringify({\n            rating: this.rating,\n            description: this.description,\n          }),\n        })\n          .then((response) => response.json())\n          .then((data) => {\n            console.log('Data successfully posted:', data);\n            // Handle the response data as needed\n            this.sucMessage = \"Thank you very much for your feedback. We are striving everyday to provide better services!\";\n            this.resetForm();\n            this.fetchReviews(); // Refresh reviews after submission\n          })\n          .catch((error) => {\n            console.error('Error posting data:', error);\n            // Handle the error\n            this.errMessage = \"An error occurred while submitting your feedback.\";\n          });\n      }\n    },\n    resetForm() {\n      this.rating = null;\n      this.description = '';\n      this.errMessage = '';\n    },\n    // Feedback Section\n    fetchReviews() {\n      axios\n        .get('https://mercury.swin.edu.au/cos30043/s104357292/HDTASK/Coffee/src/resources/api_review.php/')\n        .then((response) => {\n          this.reviews = response.data;\n        })\n        .catch((error) => {\n          console.log('Failed to load reviews:', error);\n        });\n    },\n  },\n};\n</script>\n"],"mappings":"AAkDA,OAAO,uCAAuC;AAC9C,OAAO,kCAAkC;AACzC,OAAOA,KAAI,MAAO,OAAO,EAAE;;AAE3B,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,IAAI;MACZC,WAAW,EAAE,EAAE;MACfC,UAAU,EAAE,EAAE;MACdC,UAAU,EAAE,EAAE;MACdC,OAAO,EAAE,EAAE,CAAE;IACf,CAAC;EACH,CAAC;;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,YAAY,CAAC,CAAC;EACrB,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAAA,EAAG;MAAE;MACX,IAAI,CAAC,IAAI,CAACR,MAAM,EAAE;QAChB,IAAI,CAACE,UAAS,GAAI,0BAA0B;QAC5C,OAAO,KAAK;MACd;MACA,OAAO,IAAI;IACb,CAAC;IAAG;IACJO,UAAUA,CAAA,EAAG;MACX,IAAI,IAAI,CAACD,QAAQ,CAAC,CAAC,EAAE;QACnB,MAAME,MAAK,GAAI,6FAA6F;QAC5GC,KAAK,CAACD,MAAM,EAAE;UACZE,MAAM,EAAE,MAAM;UACdC,OAAO,EAAE;YACP,cAAc,EAAE;UAClB,CAAC;UACDC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAAC;YACnBhB,MAAM,EAAE,IAAI,CAACA,MAAM;YACnBC,WAAW,EAAE,IAAI,CAACA;UACpB,CAAC;QACH,CAAC,EACEgB,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAClCF,IAAI,CAAElB,IAAI,IAAK;UACdqB,OAAO,CAACC,GAAG,CAAC,2BAA2B,EAAEtB,IAAI,CAAC;UAC9C;UACA,IAAI,CAACI,UAAS,GAAI,6FAA6F;UAC/G,IAAI,CAACmB,SAAS,CAAC,CAAC;UAChB,IAAI,CAAChB,YAAY,CAAC,CAAC,EAAE;QA