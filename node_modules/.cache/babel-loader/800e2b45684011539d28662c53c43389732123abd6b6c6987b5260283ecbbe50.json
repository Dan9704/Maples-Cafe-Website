{"ast":null,"code":"import 'bootstrap/dist/css/bootstrap.css';\nexport default {\n  name: 'ComponentOne',\n  data() {\n    return {\n      drinkItems: [],\n      foodItems: [],\n      selectedCategory: 'Drink',\n      categories: ['Food', 'Drink'],\n      currentPage: 1,\n      itemsPerPage: 6,\n      searchQuery: '',\n      priceSort: null,\n      // Default price sort option\n      priceSortOptions: ['Lowest to Highest', 'Highest to Lowest'],\n      // Feedback From Customer Section\n      loading: true,\n      reviews: []\n    };\n  },\n  computed: {\n    filteredItems() {\n      // Filter items based on search query and selected category\n      let filteredItems = this.filteredByCategory.filter(item => item.title.toLowerCase().includes(this.searchQuery.toLowerCase()));\n\n      // Sort items by price if priceSort option is selected\n      if (this.priceSort === 'Lowest to Highest') {\n        filteredItems.sort((a, b) => a.price - b.price);\n      } else if (this.priceSort === 'Highest to Lowest') {\n        filteredItems.sort((a, b) => b.price - a.price);\n      }\n      return filteredItems;\n    },\n    paginatedItems() {\n      const startIndex = (this.currentPage - 1) * this.itemsPerPage;\n      const endIndex = startIndex + this.itemsPerPage;\n      return this.filteredItems.slice(startIndex, endIndex);\n    },\n    filteredByCategory() {\n      // Filter items based on selected category\n      return this.selectedCategory === 'Drink' ? this.drinkItems : this.foodItems;\n    },\n    pageCount() {\n      return Math.ceil(this.filteredItems.length / this.itemsPerPage);\n    },\n    // Feedback From Customer Section\n    displayedReviews() {\n      // Calculate the index range for the current page\n      const startIndex = (this.currentPage - 1) * this.reviewsPerPage;\n      const endIndex = startIndex + this.reviewsPerPage;\n\n      // Slice the reviews array based on the index range\n      return this.reviews.slice(startIndex, endIndex);\n    }\n  },\n  mounted() {\n    this.loadMenuItems();\n    // Feedback From Customer Section\n    this.loadReviews();\n  },\n  methods: {\n    loadMenuItems() {\n      this.foodItems = require('@/assets/food.json');\n      this.drinkItems = require('@/assets/drink.json');\n    },\n    setCurrentPage(page) {\n      this.currentPage = page;\n    },\n    filterItems() {\n      // Triggered when user types in the search bar\n      // Automatically re-renders the displayed items\n      this.currentPage = 1; // Reset current page to 1 when filtering\n    },\n\n    clearFilters() {\n      // Reset all filter values to their default states\n      this.selectedCategory = 'Drink';\n      this.searchQuery = '';\n      this.priceSort = null;\n    },\n    // Feedback From Customer Section\n    loadReviews() {\n      axios.get('https://mercury.swin.edu.au/cos30043/s104357292/HDTASK/Coffee/src/resources/api_review.php/').then(response => {\n        this.reviews = response.data;\n        this.loading = false;\n      }).catch(error => {\n        console.log('Failed to load reviews:', error);\n        this.loading = false;\n      });\n    },\n    paginateReviews(page) {\n      this.currentPage = page;\n    }\n    /* eslint-disable */\n  }\n};","map":{"version":3,"names":["name","data","drinkItems","foodItems","selectedCategory","categories","currentPage","itemsPerPage","searchQuery","priceSort","priceSortOptions","loading","reviews","computed","filteredItems","filteredByCategory","filter","item","title","toLowerCase","includes","sort","a","b","price","paginatedItems","startIndex","endIndex","slice","pageCount","Math","ceil","length","displayedReviews","reviewsPerPage","mounted","loadMenuItems","loadReviews","methods","require","setCurrentPage","page","filterItems","clearFilters","axios","get","then","response","catch","error","console","log","paginateReviews"],"sources":["/Users/admin/Documents/HDTask/Coffee/src/components/Component1.vue"],"sourcesContent":["<template>\n    <div>\n      <!-- Dropdown button for selecting food or drink -->\n      <v-card-item class=\"my-2\">\n        <v-select v-model=\"selectedCategory\" :items=\"categories\" label=\"Select Category\"></v-select>\n      </v-card-item>\n  \n      <!-- Search bar input field -->\n      <v-text-field v-model=\"searchQuery\" label=\"Search\" outlined @input=\"filterItems\"></v-text-field>\n  \n      <!-- Dropdown box for sorting by price -->\n      <v-card-item class=\"my-2\">\n        <v-select v-model=\"priceSort\" :items=\"priceSortOptions\" label=\"Sort by Price\" outlined></v-select>\n      </v-card-item>\n  \n      <!-- Clear Filters button -->\n      <v-card-item class=\"my-2\">\n        <v-btn @click=\"clearFilters\" color=\"primary\">Clear Filters</v-btn>\n      </v-card-item>\n  \n      <v-card-item>\n        <v-card-title>Menu of Maples Cafe</v-card-title>\n        <v-card-subtitle></v-card-subtitle>\n      </v-card-item>\n      \n      <v-card-item class=\"my-2\">\n        <v-sheet>\n          <div class=\"container mx-auto align-items-center\">\n            <div class=\"row\">\n              <div class=\"col-lg-4 col-md-6 col-sm-12\" v-for=\"(item, index) in paginatedItems\" :key=\"index\">\n                <!-- First v-card-item for the image -->\n                <v-card-item class=\"m-4 p-2 elevation-2 custom-card\">\n                  <v-img :src=\"item.image\" :alt=\"item.title\" class=\"card-image\"></v-img>\n                </v-card-item>\n  \n                <!-- Second v-card-item for the title and price -->\n                <v-card-item class=\"m-4 p-2 elevation-2 custom-card\">\n                  <v-card-text class=\"text-center title-text\" style=\"font-size: 24px; color: red; font-family: 'Pacifico', cursive;\">\n                    {{ item.title }} - ${{ item.price }}\n                  </v-card-text>\n                  <v-card-text class=\"text-center\">{{ item.description }}</v-card-text>\n                </v-card-item>\n              </div>\n            </div>\n          </div>\n        </v-sheet>\n  \n        <div class=\"col-md-12\">\n          <paginate\n            :page-count=\"pageCount\"\n            :click-handler=\"setCurrentPage\"\n            :page-range=\"5\"\n            :margin-pages=\"1\"\n            :prev-text=\"'Prev'\"\n            :next-text=\"'Next'\"\n            :container-class=\"'pagination'\"\n            :page-class=\"'page-item'\"\n          ></paginate>\n        </div>\n  \n        <v-pagination v-model=\"currentPage\" :length=\"Math.ceil(filteredItems.length / itemsPerPage)\" @input=\"setCurrentPage\">\n        </v-pagination>\n      </v-card-item>\n\n      <!-- Feedback From Customer Section -->\n      <div v-if=\"loading\" class=\"text-center\"> Thank you so much for your patient we will be back soon. Loading FeedBack...</div>\n      <div v-else>\n        <v-sheet class=\"reviews-section my-5\">\n          <v-card-item class=\"text-center\">\n            <v-card-title class=\"title-secondary\">Feedback from our most-valued customers</v-card-title>\n          </v-card-item>\n          <v-card v-for=\"(review, index) in displayedReviews\" :key=\"index\" class=\"review-card m-4 p-4\">\n            <v-rating v-model=\"review.rating\" empty-icon=\"mdi-heart-outline\" full-icon=\"mdi-heart\" color=\"#74C69D\" readonly></v-rating>\n            <v-card-text class=\"review-text\">{{ review.description }}</v-card-text>\n          </v-card>\n          <v-pagination v-model=\"currentPage\" :length=\"Math.ceil(reviews.length / reviewsPerPage)\" @input=\"paginateReviews\"></v-pagination>\n        </v-sheet>\n      </div>\n    </div>\n  </template>\n  \n  <script>\n  import 'bootstrap/dist/css/bootstrap.css';\n  \n  export default {\n    name: 'ComponentOne',\n    data() {\n      return {\n        drinkItems: [],\n        foodItems: [],\n        selectedCategory: 'Drink',\n        categories: ['Food', 'Drink'],\n        currentPage: 1,\n        itemsPerPage: 6,\n        searchQuery: '',\n        priceSort: null, // Default price sort option\n        priceSortOptions: ['Lowest to Highest', 'Highest to Lowest'],\n        // Feedback From Customer Section\n        loading: true,\n        reviews: [],\n      };\n    },\n    computed: {\n      filteredItems() {\n        // Filter items based on search query and selected category\n        let filteredItems = this.filteredByCategory.filter(item =>\n          item.title.toLowerCase().includes(this.searchQuery.toLowerCase())\n        );\n  \n        // Sort items by price if priceSort option is selected\n        if (this.priceSort === 'Lowest to Highest') {\n          filteredItems.sort((a, b) => a.price - b.price);\n        } else if (this.priceSort === 'Highest to Lowest') {\n          filteredItems.sort((a, b) => b.price - a.price);\n        }\n        return filteredItems;\n      },\n\n      paginatedItems() {\n            const startIndex = (this.currentPage - 1) * this.itemsPerPage;\n            const endIndex = startIndex + this.itemsPerPage;\n            return this.filteredItems.slice(startIndex, endIndex);\n        },\n\n      filteredByCategory() {\n        // Filter items based on selected category\n        return this.selectedCategory === 'Drink' ? this.drinkItems : this.foodItems;\n      },\n      pageCount() {\n        return Math.ceil(this.filteredItems.length / this.itemsPerPage);\n      },\n\n      // Feedback From Customer Section\n      displayedReviews() {\n        // Calculate the index range for the current page\n        const startIndex = (this.currentPage - 1) * this.reviewsPerPage;\n        const endIndex = startIndex + this.reviewsPerPage;\n  \n        // Slice the reviews array based on the index range\n        return this.reviews.slice(startIndex, endIndex);\n      },\n    },\n    mounted() {\n      this.loadMenuItems();\n      // Feedback From Customer Section\n      this.loadReviews();\n    },\n    methods: {\n      loadMenuItems() {\n        this.foodItems = require('@/assets/food.json');\n        this.drinkItems = require('@/assets/drink.json');\n      },\n      setCurrentPage(page) {\n        this.currentPage = page;\n      },\n      filterItems() {\n        // Triggered when user types in the search bar\n        // Automatically re-renders the displayed items\n        this.currentPage = 1; // Reset current page to 1 when filtering\n      },\n      clearFilters() {\n        // Reset all filter values to their default states\n        this.selectedCategory = 'Drink';\n        this.searchQuery = '';\n        this.priceSort = null;\n      },\n\n      // Feedback From Customer Section\n      loadReviews() {\n        axios\n          .get('https://mercury.swin.edu.au/cos30043/s104357292/HDTASK/Coffee/src/resources/api_review.php/')\n          .then(response => {\n            this.reviews = response.data;\n            this.loading = false;\n          })\n          .catch(error => {\n            console.log('Failed to load reviews:', error);\n            this.loading = false;\n          });\n      },\n      paginateReviews(page) {\n        this.currentPage = page;\n      },\n      /* eslint-disable */\n    }\n  };\n  </script>\n  \n  <style>\n  .custom-card {\n    aspect-ratio: 1 / 1;\n  }\n  \n  @media (min-width: 600px) {\n    .row {\n      display: flex;\n      flex-wrap: wrap;\n    }\n  \n    .col-md-4 {\n      flex: 0 0 33.3333%;\n      max-width: 33.3333%;\n    }\n  }\n  </style>\n  "],"mappings":"AAkFE,OAAO,kCAAkC;AAEzC,eAAe;EACbA,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,SAAS,EAAE,EAAE;MACbC,gBAAgB,EAAE,OAAO;MACzBC,UAAU,EAAE,CAAC,MAAM,EAAE,OAAO,CAAC;MAC7BC,WAAW,EAAE,CAAC;MACdC,YAAY,EAAE,CAAC;MACfC,WAAW,EAAE,EAAE;MACfC,SAAS,EAAE,IAAI;MAAE;MACjBC,gBAAgB,EAAE,CAAC,mBAAmB,EAAE,mBAAmB,CAAC;MAC5D;MACAC,OAAO,EAAE,IAAI;MACbC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,aAAaA,CAAA,EAAG;MACd;MACA,IAAIA,aAAY,GAAI,IAAI,CAACC,kBAAkB,CAACC,MAAM,CAACC,IAAG,IACpDA,IAAI,CAACC,KAAK,CAACC,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACZ,WAAW,CAACW,