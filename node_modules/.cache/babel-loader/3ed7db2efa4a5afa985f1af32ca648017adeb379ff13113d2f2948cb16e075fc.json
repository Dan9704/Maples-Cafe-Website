{"ast":null,"code":"import 'bootstrap/dist/css/bootstrap.css';\n//   import paginate from 'vuejs-paginate';\n//     import { VuejsPaginate } from 'vuejs-paginate';\n\nexport default {\n  name: 'ComponentOne',\n  components: {\n    paginate: VuejsPaginateNext\n  },\n  data() {\n    return {\n      foodItems: [],\n      currentPage: 1,\n      itemsPerPage: 6\n    };\n  },\n  mounted() {\n    this.loadFoodItems();\n  },\n  computed: {\n    paginatedItems() {\n      const startIndex = (this.currentPage - 1) * this.itemsPerPage;\n      const endIndex = startIndex + this.itemsPerPage;\n      return this.foodItems.slice(startIndex, endIndex);\n    },\n    pageCount() {\n      return Math.ceil(this.foodItems.length / this.itemsPerPage);\n    }\n  },\n  methods: {\n    setCurrentPage(pageNumber) {\n      this.currentPage = pageNumber;\n    },\n    async loadFoodItems() {\n      try {\n        const response = await fetch('@/assets/food.json');\n        this.foodItems = await response.json();\n      } catch (error) {\n        console.error('Error loading food items:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","components","paginate","VuejsPaginateNext","data","foodItems","currentPage","itemsPerPage","mounted","loadFoodItems","computed","paginatedItems","startIndex","endIndex","slice","pageCount","Math","ceil","length","methods","setCurrentPage","pageNumber","response","fetch","json","error","console"],"sources":["/Users/admin/Documents/HDTask/Coffee/src/components/Component1.vue"],"sourcesContent":["<template>\n    <div>\n      <v-card-item>\n        <v-card-title>Menu</v-card-title>\n        <v-card-subtitle></v-card-subtitle>\n      </v-card-item>\n      <v-card-item class=\"my-2\">\n        <v-sheet>\n          <div class=\"container mx-auto align-items-center\">\n            <div class=\"row\">\n              <div class=\"col-lg-4 col-md-6 col-sm-12\" v-for=\"(item, index) in paginatedItems\" :key=\"index\">\n                <v-card max-width=\"